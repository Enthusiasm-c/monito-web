{
  "name": "vercel-marketplace-neon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:playwright": "playwright test",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:suppliers": "playwright test comprehensive-supplier-crud.spec.js",
    "test:suppliers:headed": "playwright test comprehensive-supplier-crud.spec.js --headed",
    "postinstall": "prisma generate && prisma db push --accept-data-loss",
    "validate:data": "ts-node -r tsconfig-paths/register --project tsconfig.node.json scripts/validate-data-quality.ts",
    "fix:indonesian": "ts-node -r tsconfig-paths/register --project tsconfig.node.json scripts/quick-fix-indonesian.ts",
    "fix:data": "ts-node -r tsconfig-paths/register --project tsconfig.node.json scripts/fix-existing-data.ts",
    "fix:duplicates": "ts-node -r tsconfig-paths/register --project tsconfig.node.json scripts/fix-existing-data.ts --skip-names --skip-units --skip-prices",
    "fix:all": "ts-node -r tsconfig-paths/register --project tsconfig.node.json scripts/fix-existing-data.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",

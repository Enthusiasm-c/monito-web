    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "openai": "^5.1.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.3.31",
    "playwright": "^1.53.1",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.0.0",
    "sharp": "^0.34.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"

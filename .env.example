# Database
DATABASE_URL=<your-connection-string>

# AI Services
OPENAI_API_KEY=<your-openai-api-key>

# File Storage  
BLOB_READ_WRITE_TOKEN=<your-vercel-blob-token>

# Processing Configuration
COMPLETENESS_THRESHOLD_EXCEL=0.95
COMPLETENESS_THRESHOLD_PDF=0.85
COMPLETENESS_THRESHOLD_CSV=0.98
MAX_FILE_SIZE_MB=10
LLM_FALLBACK_ENABLED=true
MAX_AI_STANDARDIZATION_PRODUCTS=20

# LLM Configuration
LLM_MODEL=gpt-4o
LLM_ENDPOINT=https://api.openai.com/v1

# Token Cost Monitoring
OPENAI_GPT4O_INPUT_COST_PER_1K=0.0025
OPENAI_GPT4O_OUTPUT_COST_PER_1K=0.01
OPENAI_GPT35_INPUT_COST_PER_1K=0.0005
OPENAI_GPT35_OUTPUT_COST_PER_1K=0.0015

# Bot API Authentication
BOT_API_KEY=<your-bot-api-key>
